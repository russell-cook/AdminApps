@model List<AdminApps.Models.ProjectScheduleVersion>


<table class="table table-striped table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().SavedAt)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SavedAt)
            </td>
            <td>
                @Html.ActionLink("View Schedule Version", "Details", new { id = item.ProjectID, scheduleVersion = item.VersionNum })
            </td>
        </tr>
    }

</table>
